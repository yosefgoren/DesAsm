curve circle:
	(cos(2*pi*t), sin(2*pi*t))

curve connect(u, v):
	u*t+v*(1-t)

curve letterO:
	connect(t, (0,0), (0,2))
	~
	connect(t, (1,0), (1,2))
	~
	connect(t, (0,0), (1,0))
	~
	connect(t, (0,2), (1,2))

curve letterL:
	connect(t, (0,2), (0,0))
	~
	connect(t, (0,0), (1,0))

curve letterA:
	connect(t, (0,0), (0,2))
	~
	connect(t, (1,0), (1,2))
	~
	connect(t, (0,2), (1,2))
	~
	connect(t, (0,1), (1,1))

curve letterF:
	connect(t, (0,0), (0,2))
	~
	connect(t, (0,2), (1,2))
	~
	connect(t, (0,1), (1,1))

curve letterH:
	connect(t, (0,0), (0,2))
	~
	connect(t, (1,0), (1,2))
	~
	connect(t, (0,1), (1,1))
